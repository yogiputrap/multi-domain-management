{
    "name": "ifitb-multidomain",
    "version": "1.0.0",
    "description": "IFITB MULTIDOMAIN - Multi-Domain Subdomain Management Platform",
    "main": "src/server.js",
    "scripts": {
        "dev": "nodemon src/server.js",
        "start": "node src/server.js",
        "db:generate": "npx prisma generate",
        "db:push": "npx prisma db push",
        "db:migrate": "npx prisma migrate dev",
        "db:seed": "node prisma/seed.js",
        "db:studio": "npx prisma studio",
        "setup": "npm install && npm run db:generate && npm run db:push && npm run db:seed",
        "build:css": "npx tailwindcss -i ./src/public/css/input.css -o ./src/public/css/output.css --watch"
    },
    "prisma": {
        "seed": "node prisma/seed.js"
    },
    "keywords": [
        "saas",
        "subdomain",
        "dns",
        "management"
    ],
    "author": "IFITB",
    "license": "MIT",
    "dependencies": {
        "@prisma/client": "^5.7.0",
        "bcrypt": "^5.1.1",
        "cloudflare": "^5.2.0",
        "cookie-parser": "^1.4.6",
        "dotenv": "^16.3.1",
        "ejs": "^3.1.9",
        "express": "^4.18.2",
        "express-rate-limit": "^7.1.5",
        "helmet": "^7.1.0",
        "jsonwebtoken": "^9.0.2",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "nodemon": "^3.0.2",
        "prisma": "^5.7.0",
        "tailwindcss": "^3.4.0"
    }
}